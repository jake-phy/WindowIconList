{
 "WindowList" : {
    "type" : "header",
    "description": "Window List Settings"
 },
 "seperator1" : {
    "type" : "separator"
 },
 "number-display" : {
    "type": "combobox",
    "default" : 2,
    "description" : "Number Display:",
    "options" : {
        "Smart" : 1,
        "normal" : 2,
        "none" : 3,
        "all" : 4
    },
    "tooltip" : "normal: display window number, smart: display window number if more than one window, none: don't display number, all: display window number for favorites too"
 },
 "title-display" : {
    "type": "combobox",
    "default" : 1,
    "description" : "Title Display:",
    "options" : {
        "none" : 1,
        "app" : 2,
        "title" : 3,
        "focused" : 4,
        "nonpinned" : 5,
        "pinned" : 6
    },
    "tooltip" : "focused: show focused window title, title: display the window title, app: diplay app name, none: don't display anything"
 },
 "pinned-apps" : {
    "type": "generic",
    "default": ["firefox.desktop","mintInstall.desktop","cinnamon-settings.desktop","xchat.desktop","gnome-terminal.desktop","nautilus.desktop","gedit.desktop"]
 },
 "pinned-recent" : {
    "type": "generic",
    "default": {}
 },
 "show-alerts" : {
    "type" : "checkbox",
    "default" : true,
    "description": "Show app alerts and notifications",
    "tooltip": "Show app alerts and notifications"
 },
 "show-pinned" : {
    "type" : "checkbox",
    "default" : true,
    "description": "Show pinned apps",
    "tooltip": "Show the pinned apps or just show the open windows"
 },
 "arrange-pinnedApps" : {
    "type" : "checkbox",
    "default" : false,
    "description": "Arrange pinned apps",
    "tooltip": "Arrange pinned apps into a different order"
 },
 "group-apps" : {
    "type" : "checkbox",
    "default" : true,
    "description": "Group apps into single icon",
    "tooltip": "Group the open app instances into single icon"
 },
 "icon-padding" : {
    "type": "spinbutton",
    "default" : 5,
    "min" : 0,
    "max" : 15,
    "step" : 1,
    "units" : "padding",
    "description" : "Icon Padding:",
    "tooltip" : "How much Padding is between the icons"
 },
